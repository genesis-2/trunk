<?xml version="1.0" encoding="utf-8" ?>
<gsh:script
	xmlns:gsh="http://vcgr.cs.virginia.edu/genii/xsh/script"
	xmlns:geniix="http://vcgr.cs.virginia.edu/genii/xsh/grid">

	<gsh:echo
		message="Creating Directory."/>
	<geniix:mkdir>
		<gsh:param>/home/mmm2a</gsh:param>
	</geniix:mkdir>

	<gsh:echo
		message="Creating Application."/>
	<geniix:app-description-tool>
		<gsh:param>--create-application</gsh:param>
		<gsh:param>/home/mmm2a/FibApplication</gsh:param>
	</geniix:app-description-tool>

	<gsh:echo
		message="Registering Jar Deployment."/>
	<geniix:app-description-tool>
		<gsh:param>--register</gsh:param>
		<gsh:param>/home/mmm2a/FibApplication</gsh:param>
		<gsh:param>Linux Jar</gsh:param>
		<gsh:param>example-dep/jar/example-jar-deployment.xml</gsh:param>
	</geniix:app-description-tool>

	<gsh:echo
		message="Registering Binary Deployment."/>
	<geniix:app-description-tool>
		<gsh:param>--register</gsh:param>
		<gsh:param>/home/mmm2a/FibApplication</gsh:param>
		<gsh:param>Linux Binary</gsh:param>
		<gsh:param>example-dep/bin/example-binary-deployment.xml</gsh:param>
	</geniix:app-description-tool>

	<gsh:echo
		message="Registering Legion-like Deployment."/>
	<geniix:app-description-tool>
		<gsh:param>--register-program</gsh:param>
		<gsh:param>/home/mmm2a/FibApplication</gsh:param>
		<gsh:param>example-dep/legion-like/FibProgram</gsh:param>
	</geniix:app-description-tool>
</gsh:script>
